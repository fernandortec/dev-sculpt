{
  "name": "@sculpt/web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --env-file=../../.env next dev",
    "build": "NODE_ENV=production bun --env-file=../../.env next build",
    "test:e2e:ui": "NEXT_PUBLIC_API_MOCKING=true bun --env-file=../../.env playwright test --ui",
